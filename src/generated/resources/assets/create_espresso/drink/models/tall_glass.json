[
  {
    "apply": "create_espresso:block/tall_glass_coffee",
    "when": "create_espresso:is_coffee"
  },
  {
    "apply": "create_espresso:block/tall_glass_bubbles",
    "when": "create_espresso:has_bubbles"
  },
  {
    "apply": "create_espresso:block/tall_glass_ice",
    "when": "create_espresso:has_ice"
  },
  {
    "apply": [
      {
        "condition": "create_espresso:is_coffee",
        "model": "create_espresso:block/tall_glass_latte"
      },
      {
        "condition": "create_espresso:has_drink",
        "model": "create_espresso:block/tall_glass_milk"
      }
    ],
    "when": "create_espresso:has_milk"
  }
]