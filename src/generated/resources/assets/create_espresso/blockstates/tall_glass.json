{
  "multipart": [
    {
      "apply": {
        "model": "create_espresso:block/tall_glass_right"
      },
      "when": {
        "chirality": "right"
      }
    },
    {
      "apply": {
        "model": "create_espresso:block/tall_glass_left"
      },
      "when": {
        "chirality": "left"
      }
    },
    {
      "apply": {
        "model": "create_espresso:block/tall_glass_drink_right"
      },
      "when": {
        "chirality": "right",
        "has_drink": "true"
      }
    },
    {
      "apply": {
        "model": "create_espresso:block/tall_glass_drink_left"
      },
      "when": {
        "chirality": "left",
        "has_drink": "true"
      }
    }
  ]
}